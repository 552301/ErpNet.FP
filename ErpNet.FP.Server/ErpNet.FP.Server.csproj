<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <AssemblyName>ErpNet.FP.Server</AssemblyName>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <NullableContextOptions>enable</NullableContextOptions>
    <NullableReferenceTypes>true</NullableReferenceTypes>
    <ApplicationIcon>ErpNet.FP.ico</ApplicationIcon>
    <StartupObject />
    <Company>ErpNet</Company>
    <Authors>ErpNet and Contributors</Authors>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <Copyright>(c) ErpNet and Contributors</Copyright>
    <AssemblyVersion>1.0.0607.1421</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="admin.css" />
    <None Remove="admin.html" />
    <None Remove="admin.js" />
    <None Remove="debug.log.1.zip" />
    <None Remove="debug.log.2.zip" />
    <None Remove="debug.log.3.zip" />
    <None Remove="debug.log.4.zip" />
    <None Remove="debug.log.5.zip" />
    <None Remove="debug.log.6.zip" />
    <None Remove="debug.log.7.zip" />
    <None Remove="debug.log.8.zip" />
    <None Remove="debug.log.9.zip" />
    <None Remove="ErpNet.FP.ico" />
    <None Remove="ErpNet.FP.thumb.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="admin.css">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="admin.html">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="admin.js">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="ErpNet.FP.ico">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="ErpNet.FP.thumb.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="3.0.0-preview5-19227-01" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ErpNet.FP.Core\ErpNet.FP.Core.csproj" />
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties BuildVersion_StartDate="2000/1/1" appsettings_1json__JsonSchema="http://json.schemastore.org/contribute" /></VisualStudio></ProjectExtensions>

</Project>
