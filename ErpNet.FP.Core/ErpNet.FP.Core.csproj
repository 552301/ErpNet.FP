<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>ErpNet.FP.Core</RootNamespace>
    <AssemblyName>ErpNet.FP.Core</AssemblyName>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <NullableContextOptions>enable</NullableContextOptions>
    <NullableReferenceTypes>true</NullableReferenceTypes>
    <PublishTrimmed>true</PublishTrimmed>
  </PropertyGroup>

    <PropertyGroup>
        <TargetLatestRuntimePatch>false</TargetLatestRuntimePatch>
    </PropertyGroup>

    <ItemGroup>
        <TrimmerRootAssembly Include="System.IO.FileSystem" />
    </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.0.0-rc1.19456.10" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3-beta1" />
    <PackageReference Include="System.IO.Ports" Version="4.6.0-rc1.19456.4" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.6.0-rc1.19456.4" />
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties BuildVersion_StartDate="2000/1/1" /></VisualStudio></ProjectExtensions>

</Project>
